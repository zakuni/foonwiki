{
  "name": "foonwiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha",
    "build": "browserify -t coffee-reactify -t debowerify --extension='.cjsx' --extension='.coffee' components/index.cjsx -o public/js/bundle.js",
    "watch": "watchify -t coffee-reactify -t debowerify -d --extension='.cjsx' --extension='.coffee' components/index.cjsx -o public/js/bundle.js -v",
    "postinstall": "bower install && npm run build"
  },
  "dependencies": {
    "bluebird": "^2.9.25",
    "body-parser": "~1.12.3",
    "bookshelf": "^0.8.0",
    "bower": "^1.4.1",
    "browserify": "^10.1.1",
    "coffee-react": "^3.2.0",
    "coffee-reactify": "^3.0.0",
    "coffee-script": "^1.9.2",
    "cookie-parser": "~1.3.4",
    "debowerify": "^1.2.1",
    "debug": "^2.1.3",
    "express": "~4.12.3",
    "jade": "~1.9.2",
    "knex": "^0.8.3",
    "morgan": "~1.5.2",
    "node-compass": "0.2.4",
    "object-inspect": "^1.0.0",
    "pg": "^4.3.0",
    "react": "^0.13.3",
    "serve-favicon": "~2.2.0",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "espower-coffee": "^0.10.0",
    "mocha": "^2.2.4",
    "power-assert": "^0.11.0",
    "watchify": "^3.2.1"
  }
}
