{
  "name": "foonwiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha",
    "build": "browserify -t coffee-reactify --extension='.cjsx' --extension='.coffee' components/index.cjsx -o public/js/bundle.js",
    "watch": "watchify -t coffee-reactify -d --extension='.cjsx' --extension='.coffee' components/index.cjsx -o public/js/bundle.js -v",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "body-parser": "~1.15.1",
    "bookshelf": "^0.9.5",
    "browserify": "^13.0.1",
    "coffee-react": "^5.0.0",
    "coffee-reactify": "^5.0.0",
    "coffee-script": "^1.9.2",
    "cookie-parser": "~1.4.3",
    "debug": "^2.1.3",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "knex": "^0.11.5",
    "morgan": "~1.7.0",
    "node-compass": "0.2.4",
    "object-inspect": "^1.0.0",
    "pg": "^5.1.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "serve-favicon": "~2.3.0",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "espower-coffee": "^1.0.1",
    "mocha": "^2.2.4",
    "power-assert": "^1.4.1",
    "watchify": "^3.2.1"
  }
}
